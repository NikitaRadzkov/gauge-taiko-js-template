{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 12
},
  "plugins": [
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "prettier",
    "airbnb-base"
  ],
  "rules": {
    "semi":"error",
    "quotes": "off",
    "no-undef": "off",
    "no-console": "off",
    "getter-return": "off",
    "class-methods-use-this": "off",
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off",
    "object-curly-newline": "off",
    "max-len": ["warn", { "code": 120 }],
    "prettier/prettier": ["warn"]
  }
}